#ifndef SRC_KEYBOARD_H_
#define SRC_KEYBOARD_H_

#include <stdint.h>

#define PS2_ADDR (0x44A10000)

enum Keys {
	ARROW_UP,
	ARROW_DOWN,
	ARROW_RIGHT,
	ARROW_LEFT,
	SPACE,
	ESC,
	RETURN,
	KEY_P,
	KEY_R
};





#endif /* SRC_KEYBOARD_H_ */
